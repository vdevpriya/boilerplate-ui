{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "This is a boiler plate code. Please customize configurations as needed.",
  "main": "index.js",
  "scripts": {
    "test": "node node_modules/.bin/mocha --compilers js:babel-register --require babel-polyfill --recursive test/**/*test.js",
    "test:integration": "node node_modules/.bin/mocha --compilers js:babel-register --require babel-polyfill --recursive test/**/*it.js",
    "test:performance": "node node_modules/.bin/mocha --compilers js:babel-register --require babel-polyfill --recursive test/**/*perf.js",
    "docker:build": "docker build -t $npm_package_name . && docker tag $npm_package_name $npm_package_name:$npm_package_version",
    "webpack": "./node_modules/.bin/webpack --config webpack.config.dev.js --display-error-details --watch --colors",
    "webpack:analyze": "./node_modules/.bin/webpack --config webpack.config.analyze.js --display-error-details --watch",
    "webpack:prod": "./node_modules/.bin/webpack --config webpack.config.prod.js --display-error-details --watch",
    "dev:server": "./node_modules/.bin/nodemon --config ./config/nodemon.json",
    "lighthouse": "./node_modules/.bin/lighthouse http://localhost:1337/ --output-path=./.lighthouse/results.out --save-assets --view",
    "analyze": "./node_modules/.bin/concurrently -p \"Analyze\" \"yarn run webpack\" \"yarn run dev:server\" \"yarn run lighthouse\"",
    "sass:app": "./node_modules/.bin/node-sass src/client/sass/app.scss -o dist/css/ --include-path src/client/sass --include-path node_modules/animatewithsass -w",
    "sass:init": "./node_modules/.bin/node-sass src/client/sass/app.scss -o dist/css/ --include-path src/client/sass --include-path node_modules/animatewithsass",
    "sass:lib": "./node_modules/.bin/node-sass src/client/sass/lib.scss -o dist/css/  --include-path node_modules/bootstrap-sass/assets/stylesheets --include-path node_modules/",
    "css:app": "./node_modules/.bin/postcss dist/css/app.css -o dist/css/app.min.css -c postcss.config.js -w",
    "css:lib": "./node_modules/.bin/postcss dist/css/lib.css -o dist/css/lib.min.css -c postcss.config.js",
    "fonts": "cp -R node_modules/bootstrap-sass/assets/fonts/bootstrap ./dist/fonts && cp node_modules/font-awesome/fonts/fontawesome* ./dist/fonts",
    "start": "./node_modules/.bin/concurrently -p \"Blucifer Build\"  \"yarn run webpack\" \"yarn run dev:server\" \"yarn run sass:init\" \"yarn run fonts\" \"yarn run sass:app & yarn run css:app\" \"yarn run sass:lib & yarn run css:lib\" -c \"blue, cyan, white.bgMagenta, yellow \" ; exit 0"
  },
  "repository": "git@orahub.oraclecorp.com:omc-apps-dev/blucifer.git",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "author": "@josiahspartz",
